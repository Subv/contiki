  <plugin>
    org.contikios.cooja.plugins.ScriptRunner
    <plugin_config>
      <script>// Run the simulation for 1000 seconds (~16 minutes)
TIMEOUT(1000000, log.log("END\n"))

// Prepare the Disturber mote type
var disturber_mote_type = sim.getMoteType("apptype1");
var disturber_mote = disturber_mote_type.generateMote(sim);

// Wait for 500 seconds
GENERATE_MSG(500000, "sleep");
YIELD_THEN_WAIT_UNTIL(msg.equals("sleep"));

// Add the Disturber mote at the specified position (Right next to Mote 3)
var position_iface = disturber_mote.getInterfaces().getPosition();
position_iface.setCoordinates(-13.805, -51.805, 0);
var id_iface = disturber_mote.getInterfaces().getMoteID();
id_iface.setMoteID(4);
sim.addMote(disturber_mote);

var disturber_added_time = sim.getSimulationTime();

YIELD_THEN_WAIT_UNTIL(msg.contains("Changed preferred parent") &amp;&amp; id == 1);

var parent_change_time = sim.getSimulationTime();

log.log(msg + "\n");
log.log('Parent changed in ' + (parent_change_time - disturber_added_time) + "\n");

// Wait until timeout
while (true) {
    YIELD(); /* wait for another mote output */
}</script>
      <active>false</active>
    </plugin_config>
    <width>600</width>
    <z>0</z>
    <height>700</height>
    <location_x>818</location_x>
    <location_y>113</location_y>
  </plugin>
